&mmc3 {
	status = "ok";

	pinctrl-names = "default";
	pinctrl-0 = <&mmc3_pins>;

	ti,bus-width = <1>;
	vmmc-supply = <&regulator_vdd18_mmc3>;

	wp-gpios = <&gpio4 25 GPIO_ACTIVE_HIGH>;	/* gpio_121 */
	cd-gpios = <&gpio4 26 GPIO_ACTIVE_HIGH>;	/* gpio_122 */
	cd-inverted;
	wp-inverted;
};

&iss {
	status = "ok";

	pinctrl-names = "default";
	pinctrl-0 = <&iss_csi21_pins>;
};
